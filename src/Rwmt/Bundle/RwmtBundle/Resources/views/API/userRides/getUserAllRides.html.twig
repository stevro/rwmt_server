{# empty Twig template #}


<table>
    <thead>
        <tr>
            <th>Id</th>
            <th>From address</th>
            <th>To address</th>
            <th>User</th>
            <th>Created at</th>
            <th>Updated at</th>

        </tr>
    </thead>
    <tbody>
        {% for ride in userRides %}
            <tr>
                <td>{{ride.rides.0.ride.fromAddress}}</td>
                <td>{{ride.rides.0.ride.fromAddress}}</td>
                <td>{{ride.rides.0.ride.toAddress}}</td>
                <td>{{ride.firstName}}</td>
                <td>{{ride.rides.0.createdAt.date}}</td>
                <td>{{ride.rides.0.updatedAt.date}}</td>
            </tr>
        {% endfor %}
    </tbody>
</table>